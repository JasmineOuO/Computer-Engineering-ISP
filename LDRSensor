int pd = 13; 
  int led = 2; 
  int senRead = 0; 
  void setup()    
 {  
  pinMode(pd,INPUT);  
  pinMode(led,OUTPUT);  
  digitalWrite(pd,HIGH);       //supply 5 volts to photodiode  
  Serial.begin(9600);  
 }  
 void loop()  
 {  
  int val=analogRead(senRead);   
  Serial.println(val);
  if(val >= 353)
  {  
   digitalWrite(led,HIGH);
  }  
  else
  {  
   digitalWrite(led,LOW);      
  }  
 }
